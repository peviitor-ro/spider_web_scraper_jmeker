              <stringProp name="script">// Get how many matches your REGEX extractor found
// In JMeter, when you use -1 match, it sets a variable `link_matchNr` with the number of matches found
def matches = vars.get(&apos;link_matchNr&apos;) as int

if (matches == 0) {
    // No links found, stop loop
    vars.put(&apos;loopControl&apos;, &apos;stop&apos;)
    log.info(&apos;No links found - stopping the loop by setting loopControl to &quot;stop&quot;&apos;)
} else {
    // Links found - continue looping
    vars.put(&apos;loopControl&apos;, &apos;loop&apos;)
