def city = vars.get(&quot;city_${k}&quot;)?.split(&apos;,&apos;)?.getAt(0)?.toString()?.replace(&apos;&quot;&apos;, &apos;\\&quot;&apos;) ?: &quot;&quot;

// Build JSON string with proper quoting and escaping
def this_job = &quot;&quot;&quot;{
