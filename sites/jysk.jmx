                <stringProp name="parameters">${__UUID}</stringProp>
                <stringProp name="script">var guid=args[0];
log.info(guid.toString());
var k = vars.get(&quot;k&quot;);
log.info(k.toString());
var title = vars.get(&quot;titles_&quot; + k);
log.info(title);
var id = vars.get(&quot;ids_&quot;+k);
var link =&quot;https://jobs.smartrecruiters.com/JYSK/&quot;+id+&quot;&quot;;
log.info(link);
var city = vars.get(&quot;cities_&quot;+k);
log.info(city);
var company = vars.get(&quot;company&quot;);
String job=&apos;{&quot;id&quot;: &quot;&apos;+guid+&apos;&quot;,&quot;job_title&quot;: &quot;&apos;+title+&apos;&quot;,&quot;job_link&quot;: &quot;&apos;+link+&apos;&quot;, &quot;company&quot;: &quot;&apos;+company+&apos;&quot;,    &quot;country&quot;: &quot;România&quot;,&quot;city&quot;: &quot;&apos;+city+&apos;&quot; }&apos;;
var joblist = vars.getObject(&quot;jobList&quot;);
joblist = joblist + job;
vars.putObject(&quot;jobList&quot;, joblist);
</stringProp>
                <stringProp name="scriptLanguage">groovy</stringProp>
              </JSR223Sampler>
